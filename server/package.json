{
  "name": "mern-server",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "start": "node -r module-alias/register dist/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "seed": "ts-node -r tsconfig-paths/register src/scripts/seed.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "_moduleAliases": {
    "@": "dist",
    "@models": "dist/models",
    "@graphql": "dist/graphql",
    "@utils": "dist/utils",
    "@services": "dist/services",
    "@validation": "dist/validation",
    "@middleware": "dist/middleware"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@graphql-tools/schema": "^10.0.30",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "graphql": "^16.12.0",
    "graphql-redis-subscriptions": "^2.7.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.16.2",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.3",
    "module-alias": "^2.2.3",
    "mongoose": "^8.20.2",
    "multer": "^2.0.2",
    "tsconfig-paths": "^4.2.0",
    "ws": "^8.18.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.27",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.4.2",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3"
  }
}